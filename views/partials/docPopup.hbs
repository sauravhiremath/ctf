    <div class="m-auto popupContainer">
        <header class="d-flex flex-row app_header" style="height: 25px">
            <div class="justify-content-start"><img src="/static/images/my_documents.png" class="header_icon" alt="">
            </div>
            <div><strong>Questions</strong></div>
            <div class="d-flex ml-auto"><button class="closeIcon"></button></div>
        </header>
        <div class="d-flex flex-row toolbar" style="height: 24px">
            <div class="nav">
                <div class="nav-item p-1 toolbar-1"><a href="#">File</a> </div>
                <div class="nav-item p-1 toolbar-1"><a href="#">Edit</a> </div>
